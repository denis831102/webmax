{
  "name": "WebMax",
  "version": "2.0.42",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prebuild": "node scripts/set-version.js",
    "build:patch": "node scripts/set-version.js patch && vue-cli-service build && vue-cli-service serve",
    "build:minor": "node scripts/set-version.js minor && vue-cli-service build && vue-cli-service serve",
    "build:major": "node scripts/set-version.js major && vue-cli-service build && vue-cli-service serve"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.5",
    "core-js": "^3.8.3",
    "element-plus": "^2.9.7",
    "lodash": "^4.17.21",
    "vue": "^3.5.18",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/no-v-model-argument": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "description": "```\r npm install\r ```",
  "main": "babel.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denis831102/webmax.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/denis831102/webmax/issues"
  },
  "homepage": "https://github.com/denis831102/webmax#readme"
}